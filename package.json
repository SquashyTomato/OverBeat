        "concurrently": "^9.2.1",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "prettier": "^3.6.2",
        "tslib": "^2.8.1",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.4"
